{
  "name": "kunlun-universal",
  "description": "My technical documatation website",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "homepage": "https://kunlun.run",
  "repository": "github:zhuyudong/universal",
  "bugs": {
    "url": "https://github.com/zhuyudong/universal/issues"
  },
  "scripts": {
    "prepare": "npx husky install",
    "prestart": "cd plugins/reference-codeblock && npm run build",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "serve:host": "docusaurus serve --host 0.0.0.0",
    "clean": "docusaurus clear",
    "prepm2:serve": "npm run build",
    "pm2:serve": "pm2 start npm -- run serve",
    "test": "npm build",
    "version:cut": "docusaurus docs:version",
    "format:source": "prettier --write {{core,src}/**/*.js,*.js}",
    "format:markdown": "prettier --write ../docs/*.md && prettier --write {{versioned_docs,src}/**/*.md,blog/*.md}",
    "format:style": "prettier --write src/**/*.{scss,css}",
    "prettier": "npm format:source && npm format:markdown && npm format:style",
    "lint": "eslint ../docs/** blog/** core/** src/**/*.{js,md} ./*.js",
    "lint:versioned": "eslint versioned_docs/**",
    "language:lint": "cd ../ && alex && case-police 'docs/*.md' -p brands,general,products,softwares",
    "language:lint:versioned": "cd ../ && alex . && case-police '**/*.md' -p brands,general,products,softwares",
    "ci:lint": "npm lint && npm lint:versioned && npm language:lint:versioned",
    "pwa:generate": "npx pwa-asset-generator ./static/img/header_logo.svg ./static/img/pwa --padding '40px' --background 'rgb(32, 35, 42)' --icon-only --opaque true",
    "image-check": "ts-node scripts/image-check.ts",
    "typecheck": "tsc"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@crowdin/cli": "^3.8.1",
    "@crowdin/crowdin-api-client": "^1.19.2",
    "@docusaurus/core": "^2.1.0",
    "@docusaurus/logger": "^2.1.0",
    "@docusaurus/plugin-client-redirects": "^2.1.0",
    "@docusaurus/plugin-content-docs": "^2.1.0",
    "@docusaurus/plugin-pwa": "^2.1.0",
    "@docusaurus/preset-classic": "^2.1.0",
    "@docusaurus/theme-classic": "^2.1.0",
    "@docusaurus/theme-common": "^2.1.0",
    "@docusaurus/utils": "^2.1.0",
    "@docusaurus/utils-common": "^2.1.0",
    "@easyops-cn/docusaurus-search-local": "^0.32.1",
    "@mdx-js/react": "1.6.22",
    "antd": "^4.23.5",
    "axios": "^1.1.2",
    "clsx": "^1.2.1",
    "docusaurus-plugin-openapi-docs": "^1.3.0",
    "docusaurus-plugin-sass": "^0.2.2",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "esbuild-loader": "^2.20.0",
    "lodash": "^4.17.21",
    "mdx-mermaid": "^1.3.2",
    "mermaid": "^9.1.7",
    "plugin-image-zoom": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.4.0",
    "remove": "^0.1.5",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.1.0",
    "@docusaurus/types": "^2.1.0",
    "@kunlunjs/fabric": "^0.4.7",
    "@svgr/plugin-prettier": "^6.5.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/eslint": "^8.4.6",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.0",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "alex": "^10.0.0",
    "chalk": "4.1.2",
    "devmoji": "^2.3.0",
    "eslint": "^8.23.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "glob-promise": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pm2": "^5.2.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.16",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  }
}
